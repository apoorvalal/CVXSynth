citHeader("To cite package 'CVXsynth' in publications use:")
     
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)
 
citEntry(entry="Manual",
		  title = "CVXSynth: Synthetic Control Using CVXR",
		  author = personList(as.person("Apoorva Lal")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Apoorva Lal (",
				year,
				"). CVXSynth: Synthetic Control Using CVXR. ",
				vers, ".", sep=""))